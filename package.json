    "test:watch": "vitest",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run -c vite-e2e.config.ts",
    "test:e2e:watch": "vitest -c vite-e2e.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
