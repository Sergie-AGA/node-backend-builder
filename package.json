    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "eslint": "^8.46.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.2.0",
    "supertest": "^6.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
